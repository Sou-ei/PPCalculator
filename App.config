<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- Add your osu! API key here. You can obtain it from https://osu.ppy.sh/p/api -->
    <add key="OsuApiKey" value="YOUR_OSU_API_KEY_HERE"/>
    <!-- Base URL for osu! API requests -->
    <add key="OsuApiBaseUrl" value="https://osu.ppy.sh/api"/>
    <!-- Timeout for HTTP requests in milliseconds -->
    <add key="HttpRequestTimeout" value="10000"/>
  </appSettings>
</configuration>
